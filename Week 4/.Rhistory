rounddelta <- round (calcdelta, 4)
calcepsilon <- epsilonFromN (N, vcdim, delta)
deltacheck <- calcdelta < delta
epsiloncheck <- calcepsilon < epsilon
data.frame(N, calcdelta, rounddelta, calcepsilon, deltacheck, epsiloncheck)
}
